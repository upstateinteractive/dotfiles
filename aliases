# Shortcuts
alias ...='../..'
alias zrc="vim ~/.zshrc"
alias vrc="vim ~/.vimrc"
alias vinstall="vim +PluginInstall +qall"
alias :q="exit"
alias wds="webpack-dev-server"

alias ga="git add -A"
alias gc="git commit"
alias gch="git checkout"
alias gl="git pull"
alias gh="git push"
alias gd="git diff"
alias gs="git status"
alias glog="git log"
alias simpleserver="python -m SimpleHTTPServer"
alias eslint="node_modules/.bin/eslint"
alias tyfa="code ~/tyfa && cd ~/tyfa && gl"
